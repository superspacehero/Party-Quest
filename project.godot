config_version=5

[addons]

resources_spreadsheet_view/color_rows=true
resources_spreadsheet_view/color_arrays=true
resources_spreadsheet_view/array_min_width=128.0
resources_spreadsheet_view/resource_preview_size=32.0
resources_spreadsheet_view/clip_headers=false
resources_spreadsheet_view/dupe_arrays=true
resources_spreadsheet_view/context_menu_on_leftclick=false

[application]

config/name="Party Quest"
run/main_scene="res://Main/Scenes/Logos.tscn"
config/features=PackedStringArray("4.2", "Mobile")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
boot_splash/image="res://Main/Art/Sprites/UI/Logo.png"
boot_splash/default_cell_height=0.25
boot_splash/default_cell_size=0.25

[autoload]

MultiplayerInput="*res://addons/multiplayer_input/multiplayer_input.gd"

[display]

window/size/viewport_width=800
window/size/viewport_height=600
window/size/mode=3
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/handheld/orientation=6

[dotnet]

project/assembly_name="Party Quest"

[editor_plugins]

enabled=PackedStringArray("res://addons/modplayer/plugin.cfg", "res://addons/multiplayer_input/plugin.cfg")

